/*
 * Design patterns that are used by Rails.
 * These patters enforce a certain style of
 * designing the project and organizing artifacts.
 */
model Rails.Patterns 

import Prelude

DesignPattern['Design Pattern'] < Concept

ActiveRecord['Active Record'] : DesignPattern
MVC['Model View Controller']  : DesignPattern

MVC.Model : Concept
MVC.Model partOf MVC

MVC.View : Concept
MVC.View partOf MVC

MVC.Controller : Concept
MVC.Controller partOf MVC



